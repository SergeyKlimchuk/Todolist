@using ToDoListApplication.Models

@model List<ApplicationUser>

<div class="list-group">
    @if (Model.Count == 0)
    {
        <button type="button" class="list-group-item disabled">Не найдено пользователей!</button>
    }
    else
    {
        foreach (ApplicationUser user in Model)
        {
            <button type="button" class="list-group-item" id="FoundUser_@user.Id">@user.Email</button>
        }
    }
</div>

